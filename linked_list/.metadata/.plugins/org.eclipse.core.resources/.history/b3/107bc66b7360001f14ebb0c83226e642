#ifndef PLATFORM_TYPE_H_
#define PLATFORM_TYPE_H_

#define Platform_8 	(8)
#define Platform_16 (16)
#define Platform_32 (32)
#define Platform_64 (64)
#define PlatformUsed (64)

#if (PlatformUsed==Platform_64)
typedef unsigned char uint8;
typedef unsigned short uint16;
typedef unsigned int uint32;
typedef unsigned long long uint64;
typedef signed char sint8;
typedef signed short sint16;
typedef signed int sint32;
typedef signed long long sint64;
typedef float float32;
typedef double float64;

#elif (PlatformUsed==Platform_32)

#elif (PlatformUsed==Platform_16)

#elif(PlatformUsed==Platform_8)



#endif /* PLATFORM_TYPE_H_ */
